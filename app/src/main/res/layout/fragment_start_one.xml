<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".kotlin.bio.fragment.StartOneFragment">


    <Button
        android:id="@+id/btn_test01"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="testBTN"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_BLE_info_01"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:textSize="20dp"
        android:gravity="center"
        android:text="@string/messageBleInfo01"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_test01"/>

    <TextView
        android:id="@+id/tv_BLE_info_02"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:textSize="18dp"
        android:gravity="center"
        android:text="@string/messageBleInfo02"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_BLE_info_01" />


</androidx.constraintlayout.widget.ConstraintLayout>