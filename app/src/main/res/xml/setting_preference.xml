<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">

    <PreferenceCategory android:title="알림 설정">
        <SwitchPreference
            android:key="alarm"
            android:defaultValue="true"
            android:summary="알림을 받으려면 체크하세요"
            android:title="알림" />

        <ListPreference
            android:key="alarm_type_list"
            android:defaultValue="1"
            android:entries="@array/array_voice"
            android:entryValues="@array/array_voice"
            android:summary="아이템1"
            android:title="벨소리 타입" />

        <ListPreference
            android:key="alarm_sound_list"
            android:defaultValue="1"
            android:entries="@array/array_voice"
            android:entryValues="@array/array_voice"
            android:summary="아이템1"
            android:title="벨소리 설정" />

    </PreferenceCategory>

    <PreferenceCategory android:title="프로필 설정">



    </PreferenceCategory>

    <PreferenceCategory android:title="언어 설정">



    </PreferenceCategory>


</PreferenceScreen>